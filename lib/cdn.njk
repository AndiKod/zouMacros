// Interactive CDN links add

{% macro pkg(pkg) %}

{# --- CSS --- #}
{% if pkg === 'bonsai' %}
    <link href="https://unpkg.com/bonsai.css@latest/dist/bonsai.min.css" rel="stylesheet">
  
  {% elif pkg === 'bulma' %}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.4/css/bulma.min.css">

  {% elif pkg === 'bootstrap' %}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

  {% elif pkg === 'chota' %}
    <link rel="stylesheet" href="https://unpkg.com/chota@latest"/>

  {% elif pkg === 'purecss' %}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/purecss@3.0.0/build/pure-min.css" integrity="sha384-X38yfunGUhNzHpBaEBsWLO+A0HDYOQi8ufWDkZ0k9e0eXz/tH3II7uKZ9msv++Ls" crossorigin="anonymous">

  {% elif pkg === 'openprops' %}
    <link rel="stylesheet" href="https://unpkg.com/open-props"/>

  {% elif pkg === 'openprops-masks' %}
    <link rel="stylesheet" href="https://unpkg.com/open-props/masks.edges.min.css"/>

  {% elif pkg === 'openprops-corners' %}
    <link rel="stylesheet" href="https://unpkg.com/open-props/masks.corner-cuts.min.css"/>

  {% elif pkg === 'openprops-normalize' %}
    <link rel="stylesheet" href="https://unpkg.com/open-props/normalize.min.css"/>

  {% elif pkg === 'openprops-buttons' %}
    <link rel="stylesheet" href="https://unpkg.com/open-props/buttons.min.css"/>
  
  {# --- Javascript --- #}
  {% elif pkg === 'alpinejs' %}
    <script src="//unpkg.com/alpinejs" defer></script>

  {% elif pkg === 'pocketbase' %}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pocketbase/0.15.1/pocketbase.umd.js" integrity="sha512-fY0Tp3wfjy2SxoxBYTnTlfsVeZq8J5/5aWY9t12zjGqAaIYPDFgWBvNZjgVkhW2UE3B8BLHsydwhkIGIM+E7yg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

  {% elif pkg === 'hyperscript' %}
    <script src="https://unpkg.com/hyperscript.org@0.9.12"></script>

  {% elif pkg === 'htmx' %}
    <script src="https://unpkg.com/htmx.org@1.9.8" integrity="sha384-rgjA7mptc2ETQqXoYC3/zJvkU7K/aP44Y+z7xQuJiVnB/422P/Ak+F/AqFR7E4Wr" crossorigin="anonymous"></script>
 
{% endif %}

{% endmacro %}